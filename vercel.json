{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/index.html",
      "dest": "/public/index.html"
    },
    {
      "src": "/download",
      "dest": "/public/download.html"
    },
    {
      "src": "/download.html",
      "dest": "/public/download.html"
    },
    {
      "src": "/about",
      "dest": "/public/about.html"
    },
    {
      "src": "/about.html",
      "dest": "/public/about.html"
    },
    {
      "src": "/help",
      "dest": "/public/help.html"
    },
    {
      "src": "/help.html",
      "dest": "/public/help.html"
    },
    {
      "src": "/pricing",
      "dest": "/public/pricing.html"
    },
    {
      "src": "/pricing.html",
      "dest": "/public/pricing.html"
    },
    {
      "src": "/checkout",
      "dest": "/public/checkout.html"
    },
    {
      "src": "/checkout.html",
      "dest": "/public/checkout.html"
    },
    {
      "src": "/email-verified",
      "dest": "/public/email-verified.html"
    },
    {
      "src": "/payment-success",
      "dest": "/public/payment-success.html"
    },
    {
      "src": "/payment-cancel",
      "dest": "/public/payment-cancel.html"
    },
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    }
  ]
}
